{
    "name": "rollerworks/search-metadata",
    "description": "Metadata extension for RollerworksSearch",
    "keywords": ["rollerworks", "search", "metadata"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/Rollerworks/rollerworks-search-metadata/contributors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "rollerworks/search": "^1.0.0-beta7",
        "rollerworks/metadata": "^0.4",
        "doctrine/annotations": "~1.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "~2.7"
    },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Component\\Search\\Metadata\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rollerworks\\Component\\Search\\Tests\\Metadata\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
